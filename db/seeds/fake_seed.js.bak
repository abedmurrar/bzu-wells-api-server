/* eslint-disable import/no-extraneous-dependencies */
const faker = require('faker');
const momentRandom = require('moment-random');

const motorCapacities = [
    '1600 T',
    '1400 T',
    '1200 T',
    '1800 T',
    '2000 T',
    '2500 T',
    '3000 T',
    '1600 CC',
    '1400 CC',
    '1200 CC',
    '1800 CC',
    '2000 CC',
    '2500 CC',
    '3000 CC'
];

const createFakeCar = () => ({
    reading: faker.random.number(),
    height:faker.random.number(),
    well_id: faker.random.number({ min: 1, max: 12 }),
    volume:faker.random.number(),
    created_at: momentRandom().format('YYYY-MM-DD hh:mm:ss')
});
exports.seed = function(knex) {
    const fakeReadings = [];
    const desiredFakeReadings = 100;
    for (let i = 0; i < desiredFakeReadings; i++) {
        fakeReadings.push(createFakeCar());
    }
    return knex('readings').insert(fakeReadings);
};
